// JQuery dom manipulation

$(document).ready(function () {
  $("#about-section").hide();
  $("#photo-section").hide();
  $("#project-section").hide();
  $("#contact-section").hide();
  $("#resume-section").hide();
  // $("footer").css({ display: "none" });
});

$("#about").click(function () {
  $("#resume-section").hide();
  $("#project-section").hide();
  $("#contact-section").hide();
  $("#photo-section").show("slow");
  $("#about-section").fadeIn("fast");
});

